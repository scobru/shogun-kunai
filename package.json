{
  "name": "shogun-yumi",
  "version": "1.0.0",
  "description": "Yumi (弓), Yari (槍) & Kunai (苦無) - P2P on GunDB. Yumi for messaging, Yari for encryption, Kunai for ephemeral file transfer. Built on Shogun Core.",
  "type": "module",
  "main": "dist/yumi.cjs.js",
  "module": "dist/yumi.esm.js",
  "browser": "dist/yumi.umd.js",
  "types": "dist/yumi.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:ts && npm run build:bundle",
    "build:ts": "tsc",
    "build:bundle": "rollup -c",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "test": "node test_umd.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "p2p",
    "peer-to-peer",
    "gundb",
    "gun",
    "decentralized",
    "encrypted",
    "webrtc",
    "browser",
    "messaging",
    "file-transfer",
    "ephemeral",
    "wormhole"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bs58": "^4.0.1",
    "bs58check": "^2.1.2",
    "node-webrtc": "^0.0.0",
    "peerjs": "^1.5.5",
    "shogun-core": "^3.3.6",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bs58": "^4.0.4",
    "@types/bs58check": "^2.1.2",
    "@types/node": "^20.11.5",
    "debug": "^4.3.4",
    "inherits": "^2.0.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tslib": "^2.6.2",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scobru/shogun-yumi.git"
  },
  "bugs": {
    "url": "https://github.com/scobru/shogun-yumi/issues"
  }
}
